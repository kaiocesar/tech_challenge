{
  "compilerOptions": {
    "target": "es2016",       
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "preserveConstEnums": true,
    "outDir": "./built"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "**/*.spec.ts", "src/__tests__/**/*"]
}
